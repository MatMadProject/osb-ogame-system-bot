<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="638.0" prefWidth="782.0" stylesheets="@../css/bot-window.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.AutoBuilderLeafTaskController">
   <children>
      <VBox prefHeight="638.0" prefWidth="782.0" spacing="2.0">
         <children>
            <HBox prefHeight="31.0" prefWidth="782.0" spacing="5.0">
               <children>
                  <ComboBox fx:id="comboBoxPlanet" prefWidth="150.0" styleClass="combo-box-autobuilder" />
                  <ComboBox fx:id="comboBoxBuilding" prefWidth="150.0" styleClass="combo-box-autobuilder" />
                  <Label contentDisplay="CENTER" prefHeight="25.0" prefWidth="75.0" text="Current level" />
                  <Label fx:id="labelCurrentLevel" contentDisplay="CENTER" prefHeight="25.0" prefWidth="75.0" styleClass="footer" textAlignment="CENTER" />
                  <Region prefHeight="25.0" prefWidth="230.0" />
                  <Button fx:id="buttonAdd" mnemonicParsing="false" prefWidth="75.0" styleClass="button-auto-builder" text="Add" />
               </children>
               <padding>
                  <Insets bottom="2.5" left="2.5" right="2.5" top="2.5" />
               </padding>
            </HBox>
            <Label alignment="CENTER" prefWidth="782.0" styleClass="footer" text="Queue" textAlignment="CENTER">
               <VBox.margin>
                  <Insets left="2.5" right="2.5" />
               </VBox.margin>
            </Label>
            <ScrollPane prefHeight="282.0" prefWidth="782.0">
               <content>
                  <VBox fx:id="vBoxQueue" prefWidth="777.0" />
               </content>
            </ScrollPane>
            <Label alignment="CENTER" layoutX="10.0" layoutY="43.0" prefWidth="782.0" styleClass="footer" text="History" textAlignment="CENTER">
               <VBox.margin>
                  <Insets left="2.5" right="2.5" />
               </VBox.margin>
            </Label>
            <ScrollPane prefHeight="282.0" prefWidth="782.0">
               <content>
                  <VBox fx:id="vBoxHistory" prefWidth="777.0" />
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
